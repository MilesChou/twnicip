{
    "name": "mileschou/twnicip",
    "description": "Twnic IP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.1 | ^8.0",
        "mileschou/ip": "^0.2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "phpbench/phpbench": "^0.16 | ^1.1",
        "phpunit/phpunit": "^7.5 | ^8.5 | ^9.6",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/console": "^4 | ^5",
        "symfony/css-selector": "^4 |^5",
        "symfony/dom-crawler": "^4 | ^5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\TwnicIp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
