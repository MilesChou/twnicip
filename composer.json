{
    "name": "mileschou/twnicip",
    "description": "Twnic IP",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.0",
        "mileschou/ip": "^0.2.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "phpbench/phpbench": "^1.2",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/console": "^6.0",
        "symfony/css-selector": "^6.0",
        "symfony/dom-crawler": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\TwnicIp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
